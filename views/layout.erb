    <script>
      $(document).ready(function() {
      $('#container').waypoint(function(event, direction) {
      $('yield').toggleClass('sticky', direction === "down");
      });
      $('#fancy').waypoint(function(event, direction) {
      $(this).parent().toggleClass('sticky', direction === "down");
      $('yield').toggleClass('bumpit', direction === "down");
      event.stopPropagation();
      });});
    </script>    

    <script>
      function show(thing) {
        if (thing == 'diy') {
          var opposite = ['premix','premix_dump','premix_target'];
	} else if (thing == 'premix') {
          var opposite = ['diy', 'solution', 'dump', 'target'];
	} else if (thing == 'solution') {
          var opposite = ['pies'];
	} else {
          var opposite = ['solution'];
	}
        var set = document.getElementsByName(thing);
        for (var i = 0; i < set.length; i++) {
          set[i].style.display='block';
	}
	for (var x =0; x < opposite.length; x++) {
          var oppset = document.getElementsByName(opposite[x]);
          for (var i = 0; i < oppset.length; i++) {
            oppset[i].style.display='none';
          }
	}
      } 
    </script>
